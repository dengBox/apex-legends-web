{
  "name": "apex-legends-web",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "author": "dengbo <1716775221@qq.com>",
  "scripts": {
    "serve": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint --ext .js,.jsx,.vue src --fix",
    "lint:staged": "lint-staged",
    "cz": "npm run log && git add . && git cz",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "commitlint": "commitlint --config commitlint.config.mjs -e -V"
  },
  "dependencies": {
    "axios": "^1.6.3",
    "element-plus": "^2.4.4",
    "pinia": "^2.1.7",
    "sass": "^1.69.7",
    "unocss": "^0.58.3",
    "vue": "^3.4.3",
    "vue-axios": "^3.5.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "commitizen": "^4.3.0",
    "commitlint": "^17.7.1",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.56.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vue-cli-plugin-commitlint": "^1.0.12",
    "vue-tsc": "^1.8.27"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  }
}
